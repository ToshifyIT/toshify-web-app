{
  "name": "toshify-web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "PORT=3001 node server.js",
    "build": "tsc -b && vite build",
    "start": "node server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "sync:cabify": "npx tsx scripts/sync-cabify-historical.ts",
    "sync:cabify:weekly": "npx tsx scripts/sync-cabify-historical.ts weekly 1",
    "sync:cabify:backfill": "npx tsx scripts/sync-cabify-historical.ts backfill 12",
    "sync:cabify:realtime": "npx tsx scripts/sync-cabify-historical.ts realtime",
    "check:cabify-historico": "npx tsx scripts/check-cabify-historico.ts",
    "sync:uss": "npx tsx scripts/sync-uss-kilometraje.ts",
    "sync:uss:dry": "npx tsx scripts/sync-uss-kilometraje.ts --dry-run",
    "sync:uss:excesos": "npx tsx scripts/backfill-uss-excesos.ts",
    "sync:uss:excesos:week": "npx tsx scripts/backfill-uss-excesos.ts --days 7",
    "sync:wialon:bitacora": "npx tsx scripts/sync-wialon-bitacora-direct.ts",
    "sync:wialon:bitacora:realtime": "npx tsx scripts/sync-wialon-bitacora-realtime.ts",
    "sync:wialon:bitacora:once": "npx tsx scripts/sync-wialon-bitacora-realtime.ts --once",
    "deploy:cron": "bash scripts/deploy-cron-jobs.sh",
    "scheduler": "npx tsx scripts/scheduler-daemon.ts",
    "scheduler:pm2": "pm2 start scripts/scheduler-daemon.ts --name toshify-scheduler --interpreter npx --interpreter-args tsx"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "express": "^4.21.2",
    "googleapis": "^144.0.0",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.548.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-easy-crop": "^5.5.6",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.5.1",
    "sweetalert2": "^11.26.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "xlsx": "^0.18.5"
  }
}
